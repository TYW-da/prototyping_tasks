<mujoco>
    <option timestep="0.0002" />
    <asset>
        <material name="black" rgba="0 0 0 0.5" />
        <material name="blue_" rgba="0 0 1 1" />
        <material name="green" rgba="0 1 0 1" />
        <material name="red__" rgba="1 0 0 1" />
        <material name="white" rgba="1 1 1 1" />
        <material name="trans" rgba="0 0 0 0" />
    </asset>
    <actuator>
        <motor gear="100" joint="joint_1" name="first_motor" ctrlrange="-50 50" ctrllimited="true"/>
        <motor gear="100" joint="joint_5" name="second_motor" ctrlrange="-50 50" ctrllimited="true"/>
    </actuator>
    <worldbody>
        <geom type="plane" size="10 10 0.1" pos="0 0 0" rgba=".9 0 0 1" />
        <light diffuse=".5 .5 .5" pos="0 0 3" dir="0 0 -1" />
        <body name="base" pos="0 0 0.15" euler="0 90 0">
            <geom type="box" size=".1 .1 1.25" pos="0 0 1.35" euler="0 0 0" material="trans" />
        </body>
        <body name="active_0" pos="0 0 0.15" euler="0 100 0">
            <geom type="cylinder" size=".15 .12" pos="0 0 0" euler="90 0 0" material="blue_" />
            <joint name="joint_1" pos="0 0 0" axis="0 1 0" />-
            <geom type="box" size=".1 .1 .5" pos="-.6 0 0" euler="0 90 0" material="white" />
            <body name="passive_0" pos="-1.2 0 0" euler="0 55 0">
                <joint name="joint_2" pos="0 0 0" axis="0 1 0" />-
                <geom type="cylinder" size=".15 .12" pos="0 0 0" euler="90 0 0" material="blue_" />
                <geom type="box" size=".1 .1 .5" pos="-.6 0 0" euler="0 90 0" material="white" />
                 <body name="passive_1" pos="-1.2 0 0" euler="0 50 0" gravcomp="10">
                    <joint name="joint_3" pos="0 0 0" axis="0 1 0" />-
                    <geom type="cylinder" size=".15 .12" pos="0 0 0" euler="90 0 0" material="red__" />
                    <geom type="box" size=".1 .1 .5" pos="-.6 0 0" euler="0 90 0" material="white" />
                    <body name="passive_2" pos="-1.2 0 0" euler="0 55 0">
                        <joint name="joint_4" pos="0 0 0" axis="0 1 0" />-
                        <geom type="cylinder" size=".15 .12" pos="0 0 0" euler="100 0 0" material="blue_" />
                        <geom type="box" size=".1 .1 .5" pos="-.6 0 0" euler="0 90 0" material="white" />
                        <body name="active_1" pos="-1.2 0 0" euler="0 45 0">
                            <joint name="joint_5" pos="0 0 0" axis="0 1 0" />-
                            <geom type="cylinder" size=".15 .12" pos="0 0 0" euler="90 0 0" material="blue_" />
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>
    <equality>
        <connect name="link_0" active="true" body1="active_0" body2="base" anchor="0 0 0"/>
        <connect name="link_1" active="true" body1="active_1" body2="base" anchor="0 0 0"/>
    </equality>
    <contact>
        <exclude body1="active_0" body2="base"/>
        <exclude body1="passive_2" body2="base"/>
        <exclude body1="passive_0" body2="base"/>
    </contact>
</mujoco>

